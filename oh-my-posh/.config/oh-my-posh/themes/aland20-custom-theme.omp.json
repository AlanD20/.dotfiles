{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "console_title_template": "{{.UserName}}@{{.HostName}} {{.Shell}} in {{.PWD}}",
  "final_space": true,
  "version": 2,
  "palette": {
    "os": "p:blue",
    "bg-dark": "#242424",
    "bg-lightdark": "#202020",
    "bg-light": "#575656",
    "bg-primary": "#6272a4",
    "bg-secondary": "#516BEB",
    "text-dark": "#000000",
    "text-lightdark": "#ACB0BE",
    "text-light": "#ffffff",
    "success": "#33DD2D",
    "warning": "#FFBB00",
    "danger": "#f1184c",
    "git-unchanged": "p:success",
    "git-changed": "p:warning",
    "git-ahead": "#C792EA",
    "git-behind": "#C792EA",
    "git-diverged": "#c5e478",
    "blue": "#3A86FF"
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "os",
          "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}}<#262626> </>",
          "background": "p:bg-dark",
          "foreground": "p:os",
          "style": "diamond",
          "leading_diamond": "",
          "properties": {
            "linux": "",
            "macos": "",
            "windows": ""
          }
        },
        {
          "type": "root",
          "template": "<parentBackground></>  ",
          "background": "p:danger",
          "foreground": "p:text-dark",
          "style": "powerline",
          "powerline_symbol": ""
        },
        {
          "type": "session",
          "template": " {{ .UserName }} ",
          "background": "p:bg-secondary",
          "foreground": "p:text-light",
          "style": "powerline",
          "powerline_symbol": ""
        },
        {
          "type": "path",
          "template": " {{ .Path }} ",
          "background": "p:bg-primary",
          "foreground": "p:text-light",
          "style": "powerline",
          "powerline_symbol": "",
          "properties": {
            "folder_icon": "  ",
            "folder_separator_icon": "  ",
            "home_icon": "",
            "style": "full"
          },
          "mapped_locations": {
            "~/.dotfiles": " ",
            "~/dev": "󰵮 ",
            "~/.config": " ",
            "~/db": " ",
            "~/Documents": "󱔗 ",
            "~/Downloads/": " ",
            "~/pictures/": " ",
            "~/temp": "󱫬 "
          }
        },
        {
          "type": "git",
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }}   {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }}<p:danger>   {{ .Staging.String }}</>{{ end }} ",
          "background": "p:bg-dark",
          "foreground": "p:git-unchanged",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:git-changed{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:git-diverged{{ end }}",
            "{{ if gt .Ahead 0 }}p:git-ahead{{ end }}",
            "{{ if gt .Behind 0 }}p:git-behind{{ end }}"
          ],
          "style": "powerline",
          "powerline_symbol": "",
          "properties": {
            "github_icon": "  ",
            "gitlab_icon": "  ",
            "bitbucket_icon": "  ",
            "azure_devops_icon": "  ",
            "git_icon": " ",
            "branch_icon": " ",
            "cherry_pick_icon": " ",
            "commit_icon": " ",
            "merge_icon": " ",
            "no_commits_icon": " ",
            "rebase_icon": " ",
            "revert_icon": " ",
            "tag_icon": " ",
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "fetch_bare_info": true
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        // {
        //   "type": "upgrade",
        //   "template": "   {{ .Version }}",
        //   "foreground": "p:bg-dark",
        //   "background": "p:warning",
        //   "style": "powerline",
        //   "powerline_symbol": "",
        //   "invert_powerline": true
        // },
        {
          "type": "executiontime",
          "template": "  {{ .FormattedMs }}\u2800",
          "background": "p:bg-light",
          "foreground": "p:text-light",
          "style": "powerline",
          "powerline_symbol": "",
          "invert_powerline": true,
          "properties": {
            "always_enabled": true,
            "style": "roundrock",
            "threshold": 0
          }
        },
        {
          "type": "root",
          "template": " ⚡",
          "background": "#86BBD8",
          "foreground": "#FFD700",
          "style": "powerline",
          "powerline_symbol": ""
        },
        {
          "type": "root",
          "template": " ⚡",
          "background": "#86BBD8",
          "foreground": "#FFD700",
          "style": "powerline",
          "powerline_symbol": ""
        },
        {
          "type": "sysinfo",
          "template": " {{ .Code }} ",
          "background": "p:bg-lightdark",
          "foreground": "#ffffff",
          "style": "diamond",
          "trailing_diamond": "",
          "invert_powerline": true
        }
      ]
    },
    {
      "type": "rprompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "cmake",
          "template": "   cmake {{ .Full }} ",
          "foreground": "#E8EAEE",
          "background": "#1E9748",
          "style": "powerline",
          "powerline_symbol": ""
        },
        {
          "type": "node",
          "template": " {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
          "background": "p:bg-dark",
          "foreground": "p:success",
          "style": "powerline",
          "powerline_symbol": "",
          "properties": {
            "fetch_package_manager": true,
            "npm_icon": " <#cc3a3a></> ",
            "yarn_icon": " <#348cba></>"
          }
        },
        {
          "type": "php",
          "template": "󰌟 {{ .Full }} ",
          "foreground": "#ffffff",
          "background": "p:blue",
          "style": "powerline",
          "powerline_symbol": ""
        },
        {
          "type": "python",
          "template": "  {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }}",
          "background": "#306998",
          "foreground": "#FFE873",
          "style": "powerline",
          "powerline_symbol": ""
        },
        {
          "type": "java",
          "template": "  {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "background": "p:bg-dark",
          "foreground": "#ec2729",
          "style": "powerline",
          "powerline_symbol": ""
        },
        {
          "type": "dotnet",
          "template": "   {{ if .Unsupported }}{{ else }}{{ .Full }}{{ end }}",
          "background": "p:bg-dark",
          "foreground": "#0d6da8",
          "style": "powerline",
          "powerline_symbol": ""
        },
        {
          "type": "go",
          "template": " 󰟓  {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "background": "p:bg-dark",
          "foreground": "#06aad5",
          "style": "powerline",
          "powerline_symbol": ""
        },
        {
          "type": "rust",
          "template": "  {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "background": "p:bg-dark",
          "foreground": "#925837",
          "style": "powerline",
          "powerline_symbol": ""
        },
        {
          "type": "dart",
          "template": "  {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "background": "p:bg-dark",
          "foreground": "#055b9c",
          "style": "powerline",
          "powerline_symbol": ""
        },
        {
          "type": "nx",
          "template": " 󱄅 {{ if .Error }}{{ .Error }}{{ else }}Nx {{ .Full }}{{ end }}",
          "background": "p:bg-dark",
          "foreground": "#ffffff",
          "style": "powerline",
          "powerline_symbol": ""
        },
        {
          "type": "aws",
          "template": " 󰸏 {{.Profile}}{{if .Region}}@{{.Region}}{{end}}",
          "background": "p:bg-dark",
          "foreground": "#faa029",
          "style": "powerline",
          "powerline_symbol": ""
        },
        {
          "type": "kubectl",
          "template": " 󱃾 {{.Context}}{{if .Namespace}} :: {{.Namespace}}{{end}}",
          "background": "#316ce4",
          "foreground": "#ffffff",
          "style": "powerline",
          "powerline_symbol": ""
        },
        {
          "type": "docker",
          "style": "powerline",
          "powerline_symbol": "",
          "foreground": "p:bg-dark",
          "background": "#0B59E7",
          "template": "   {{ .Context }} "
        },
        {
          "type": "text",
          "template": "  ",
          "background": "p:bg-dark",
          "foreground": "p:warning",
          "style": "diamond",
          "trailing_diamond": ""
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "text",
          "template": "\u2570\u2500",
          "foreground": "#21c7c7",
          "style": "plain"
        },
        {
          "type": "status",
          "template": " ",
          "foreground": "#e0f8ff",
          "foreground_templates": ["{{ if gt .Code 0 }}#ef5350{{ end }}"],
          "style": "plain",
          "properties": {
            "always_enabled": true
          }
        }
      ]
    }
  ],
  "secondary_prompt": {
    "background": "transparent",
    "foreground": "#ffffff",
    "template": "-> "
  },
  "debug_prompt": {
    "background": "transparent",
    "foreground": "#ffffff",
    "template": "Debugging "
  },
  "transient_prompt": {
    "background": "transparent",
    "foreground": "#ffffff",
    "template": "  {{ .PWD }}  "
  }
}
